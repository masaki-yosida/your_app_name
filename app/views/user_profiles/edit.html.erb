<!-- app/views/user_profiles/edit.html.erb -->

<h1>Edit User Profile</h1>

<%= form_for @user_profile, url: user_profile_path(@user_profile), method: :patch, html: { multipart: true } do |f| %>
  <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <!-- Add other user profile attributes with similar fields -->

  <%= f.submit "Update Profile" %>
<% end %>

<%= link_to 'Back to Profile', user_profile_path(@user_profile) %>
